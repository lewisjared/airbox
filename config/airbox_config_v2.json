    {
      "name": "Ozone",
      "node": "ozone",
      "mount_name": "iPort",
      "filter": "49i 112718 2056 AA.dat"
    },
    {
      "name": "Spectronus",
      "node": "spectronus",
      "mount_name": "Data",
      "path": "1810_AuroraAustralis_V2/"
    },
    {
      "name": "Tekran",
      "node": "tekran",
      "mount_name": "Data",
      "path": "v1/"
    },
    {
      "name": "MAXDOAS",
      "node": "maxdoas",
      "mount_name": "Data",
      "path": "v2_Dec2018/",
      "extra": "--include=*.tar.gz --exclude=SP??????"
    },
    {
      "name": "MiniMPL",
      "node": "minimpl",
      "mount_name": "data",
      "filter": "*.mpl"
    },
    {
      "name": "Radon",
      "node": "ansto",
      "mount_name": "AIRBOX"
    },
    {
      "name": "WeatherStation",
      "node": "ansto",
      "mount_name": "UMB-Config",
      "filter": "*Values.txt"
    },
    {
      "name": "CPC3776",
      "node": "csiro",
      "mount_name": "Data",
      "path": "201819_CAMMPCAN/CPC3776/v2/"
    },
    {
      "name": "CPC3772",
      "node": "csiro",
      "mount_name": "Data",
      "path": "201819_CAMMPCAN/CPC3772/v2/"
    },
    {
      "name": "SMPS",
      "node": "csiro",
      "mount_name": "Data",
      "path": "201819_CAMMPCAN/SMPS/v2/"
    },
    {
      "name": "Neph",
      "node": "csiro",
      "mount_name": "Data",
      "path": "201819_CAMMPCAN/Neph/v2/",
      "filter": "*.txt"
    },
    {
      "name": "AerosolTempRH",
      "node": "csiro",
      "mount_name": "Data",
      "path": "HMP/",
      "filter": "*.log"
    },
    {
      "name": "CCN",
      "node": "ccn",
      "mount_name": "dmt",
      "path": "CCN Data/",
      "extra": "--exclude=2018_AA --exclude=CAMPCAN_UTC10 --delete"
    },
    {
      "name": "ACSM",
      "node": "acsm",
      "mount_name": "Data2",
      "path": "CAMMPCAN/ACSM/V2/"
    },
    {
      "name": "MAAP",
      "node": "acsm",
      "mount_name": "Data2",
      "path": "CAMMPCAN/MAAP/",
      "extra": "--exclude V*"
    },
    {
      "name": "CIMS",
      "node": "cims",
      "mount_name": "Data",
      "path": "V2/"
    },
    {
      "name": "NAIS",
      "node": "nais",
      "mount_name": "nais",
      "path": "NAIS-Aurora",
      "extra": "--exclude V*"
    }
  ],
  "schedule": [
    {
      "command": "backup",
      "frequency": "h"
    },
    {
      "command": "basic_plot",
      "frequency": "d",
      "args": {
        "dump_timeseries": true
      }
    },
    {
      "command": "backup_sync",
      "frequency": "d"
    }
  ],
